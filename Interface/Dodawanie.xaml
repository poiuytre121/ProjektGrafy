<Window x:Class="Interface.Dodawanie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interface"
        mc:Ignorable="d"
        Title="Dodawanie" Height="300" Width="400">
    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <StackPanel>
                <TextBlock Text="Podaj sąsiadów poszczególnych wierzchołków po przecinkach." Margin="5" FontSize="13"></TextBlock>
                <StackPanel x:Name="Panel"></StackPanel>
                <Button HorizontalAlignment="Center" Width="100" Height="50" FontSize="20" Content="Zatwierdź" Margin="5" Click="Button_Click"></Button>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Window>
